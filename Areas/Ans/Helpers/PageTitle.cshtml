@{
	var parents1 = (Current.IsOn("PageTitle-Parents"))
		? Current.Page.ParentsTitles
			.Make("<span class=\"opacity-75 fw-light\">{0}.</span> ")
		: null;
}
<h1 class="my-3">@parents1.ToHtml()@Current.Page.TitleHtml</h1>